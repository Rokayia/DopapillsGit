                android:value="com.example.dopapillsgit.UserActivity" />
        </activity>
        <activity android:name=".AjoutSymptomeActivity"
            android:parentActivityName=".UserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.UserActivity" />
