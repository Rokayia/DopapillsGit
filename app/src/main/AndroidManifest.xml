<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dopapillsgit">
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ConnexionActivity"
            android:parentActivityName="com.example.dopapillsgit.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.MainActivity" />
        </activity>
        <activity android:name=".InscriptionActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.MainActivity" />
        </activity>
        <activity android:name=".BienvenueActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.MainActivity" />
        </activity>
        <activity android:name=".VousEtesActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.MainActivity" />
        </activity>

        <activity android:name=".IdentifiantsCompteActivity"
            android:parentActivityName=".UserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.UserActivity" />
        </activity>
        <activity android:name=".DonnesSanteActivity"
            android:parentActivityName=".UserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.UserActivity" />
        </activity>
        <activity android:name=".MedicamentActivity"
            android:parentActivityName=".UserActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.dopapillsgit.UserActivity" />
        </activity>
        <activity android:name=".MedecinActivity"
            android:parentActivityName=".UserActivity">
